package com.ty;

public class Car {
	
	String brand;
	
	Engine engine;

	public void setBrand(String brand) {
		this.brand = brand;
	}

	public void setEngine(Engine engine) {
		this.engine = engine;
	}
	
	public void drive () {
		System.out.println("Brand : "+brand);
		System.out.println("Engine : "+engine.cc);
		engine.start();
	}
}
