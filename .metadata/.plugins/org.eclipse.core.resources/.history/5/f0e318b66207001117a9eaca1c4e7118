package com.ty.di;

import org.springframework.stereotype.Component;

@Component
public class Person {
	
	String name;
	int age;
	
	public void details() {
		System.out.println("Name :"+name);
		System.out.println("Age :"+age);
	}
	
	public void read() {
		System.out.println("person id reading a book");
	}
}
